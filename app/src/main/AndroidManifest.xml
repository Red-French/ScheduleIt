<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="net.redfrench.scheduleit">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".BookingActivity">
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="59537b9b4dcadc61a6ff0a3a8550bd2326009afa"/>

        <activity
            android:name=".MyAppointmentsActivity"
            android:label="@string/title_activity_my_appointments"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>